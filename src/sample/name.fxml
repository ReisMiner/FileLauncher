<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="124.0" prefWidth="293.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.chooserController">
   <children>
      <Button fx:id="name" layoutX="29.0" layoutY="85.0" mnemonicParsing="false" onAction="#giveName" prefHeight="25.0" prefWidth="228.0" text="OK" />
      <Label layoutX="29.0" layoutY="14.0" text="Give a name" />
      <TextField fx:id="name_field" layoutX="29.0" layoutY="46.0" prefHeight="25.0" prefWidth="228.0" onAction="#giveName"/>
   </children>
   <cursor>
      <Cursor fx:constant="DEFAULT" />
   </cursor>
</AnchorPane>
